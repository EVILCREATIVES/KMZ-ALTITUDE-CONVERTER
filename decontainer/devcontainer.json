{
  "name": "KMZ Altitude Injector",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "postCreateCommand": "pip install -r requirements.txt",
  "forwardPorts": [
    8501
  ],
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "portsAttributes": {
    "8501": {
      "label": "Streamlit",
      "onAutoForward": "openBrowser"
    }
  }
}
